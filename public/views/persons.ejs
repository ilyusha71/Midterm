<div class = "role">	
	<div class="clear"></div>
	<div class = "left_role">
	<h1>Gingerbread Man</h1>
		<% if (userinfos.length) { %>
		<ul>
			<% userinfos.forEach(function(userinfo){ %>
			<li>
				<% if (userinfo['character']== 'Gingerbread Man') { %>
				<div class='name'>
					<%= userinfo['profession'] %>
				</div>
				<a href="" class='read' rel=<%= userinfo['_id']%>>000</a>
				<a href="" class='delete' rel=<%= userinfo['_id']%>>DEL</a>
				<a href="" class='update' rel=<%= userinfo['_id']%>>EDIT</a>
			<% } %>
			</li>
			<% }) %>
		</ul>
		<% } %>
		</div>
		<div class = "mid_role">222</div>
		<div class = "right_role">333</div>
	<div class="clear"></div>
</div>

		

		
				

	
			